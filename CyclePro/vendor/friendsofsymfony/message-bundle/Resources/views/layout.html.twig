{%extends "base.html.twig" %}
{% block content %}
    <br><br><br><br><br><br><br><br>
    <!DOCTYPE html>
    <html>
    <head>
        <meta http-equiv="Content-Type" content="text/html; charset={{ _charset }}" />
        <title></title>
    </head>

    <body>
    <div class="container">
        <div class="col-12 grid-margin stretch-card">
            <div class="card">
                <div class="card-body">
                    <form>
                        <h1><center>{% trans from 'FOSMessageBundle' %}messenger{% endtrans %}</center></h1>
                        <div class="form-group">
                            <div class="col-sm-9">
                                <a href="{{ url('fos_message_inbox') }}">{% trans from 'FOSMessageBundle' %}inbox{% endtrans %} </a>
                            </div>
                        </div>
                        <div class="form-group">
                            <div class="col-sm-9">
                                <a href="{{ url('fos_message_sent') }}">{% trans from 'FOSMessageBundle' %}sent{% endtrans %}</a>
                            </div>
                        </div>
                        <div class="form-group">
                            <div class="col-sm-9">
                                <a href="{{ url('fos_message_deleted') }}">{% trans from 'FOSMessageBundle' %}deleted{% endtrans %}</a>
                            </div>
                        </div>
                        <div class="form-group">
                            <div class="col-sm-9">
                                {% block fos_message_content %}{% endblock %}
                            </div>
                        </div>

                    </form>
                </div>
            </div>
        </div>
    </div>

    </div>
    </body>
    </html>
<br><br><br><br><br><br><br><br>
{% endblock %}